<%= render 'users/new_user', user: @user %>
<%= render 'books/new_book', book: @book %>

<h2>Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td>
        <%= image_tag user.get_profile_image(100,100) %>
      </td>
      <td>
        <%= user.name %>
      </td>
      <td>
        <%= link_to "show", user_path(user.id) %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>